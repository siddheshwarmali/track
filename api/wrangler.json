{
  "name": "cycle-cipher",
  "main": "worker.js",
  "compatibility_date": "2024-09-23",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": ".",
    "binding": "ASSETS"
  },
  "vars": {
    "GITHUB_OWNER": "your-github-username",
    "GITHUB_REPO": "your-repo-name",
    "GITHUB_TOKEN": "your-github-token"
  },
  "observability": {
    "enabled": true
  }
}
